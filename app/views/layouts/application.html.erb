<!DOCTYPE html>
<html>
  <head>
    <title>SoundClown</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'player', 'data-turbolinks-track': 'reload' %>


  </head>

  <body>
    <%= render "shared/navbar" %>
